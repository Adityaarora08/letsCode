#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin >> n;
    int k;
    cin >> k;
    vector<int> arr(n);
    for (int i = 0; i < n; i++)
        cin >> arr[i];

    priority_queue<int> pq;
    for (int i = 0; i < n; i++)
        pq.push(arr[i]);

    int l = k - 1;
    while (l > 0)
    {
        pq.pop();
        l--;
    }

    cout << pq.top();
}
